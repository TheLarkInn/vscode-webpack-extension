{
  "name": " ⛱ VSCode Sandbox ⛱",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@types/async": "^2.0.49",
    "@types/node": "^10.5.1",
    "@types/puppeteer": "^1.5.0",
    "@types/webpack": "^4.4.7",
    "async": "^2.6.1",
    "azure-storage": "^2.10.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "get-chrome": "^0.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.1",
    "puppeteer": "^1.6.0-next.1532381444032",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "typescript": "^3.1.0-dev.20180721",
    "url-loader": "^1.0.1",
    "vscode": "^1.1.18",
    "webpack": "^4.13.0",
    "webpack-merge": "^4.1.3",
    "webpack-serve": "^1.0.4",
    "which-chrome": "^0.1.1"
  },
  "publisher": "Sean Larkin <sean.larkin@microsoft.com>",
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:extension.sayHello",
    "onCommand:extension.deploy",
    "workspaceContains:**/webpack.config.js",
    "workspaceContains:./src/index.js",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.webpackBuild",
        "title": "Webpack Build"
      },
      {
        "command": "extension.webpackServe",
        "title": "Webpack Serve"
      },
      {
        "command": "extension.deploy",
        "title": "Deploy build and launch browser"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "builtModulesView",
          "name": "Built Modules Explorer"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "devDependencies": {
    "vscode-languageclient": "^4.2.1",
    "vscode-languageserver": "^4.2.1"
  }
}
